{
  "name": "node-cron-decorator",
  "version": "0.0.3",
  "description": "Typescript decorator for node-cron scheduled tasks.",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/arvyylabs/node-cron-decorator.git"
  },
  "author": "vundyalaavinash <vundyala.avinash@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/arvyylabs/node-cron-decorator/issues"
  },
  "tags": [
    "typescript",
    "node-cron",
    "scheduledTask",
    "task"
  ],
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc",
    "build:all": "rimraf ./lib && rimraf ./dist && yarn build && yarn build:test",
    "build:test": "tsc -project ./test/tsconfig.json",
    "start:test": "yarn build:test && node --inspect ./dist/server.js"
  },
  "dependencies": {
    "glob": "^7.1.6",
    "node-cron": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "tz-offset": "^0.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@types/node-cron": "^2.0.3",
    "@types/express": "^4.17.2",
    "@types/moment": "^2.13.0",
    "@types/node": "^12.12.17",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "copyfiles": "^2.1.1",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "express": "^4.17.1",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "typedi": "^0.8.0",
    "typescript": "^3.7.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,ts,css,json,md,yml}": [
      "prettier --write"
    ],
    "*.{js,ts}": [
      "eslint --fix"
    ]
  },
  "homepage": "https://github.com/arvyylabs/node-cron-decorator#readme",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "keywords": [
    "typescript"
  ]
}
